{
  "all": true,
  "check-coverage": true,
  "lines": 60,
  "functions": 60,
  "branches": 60,
  "statements": 60,
  "reporter": ["text", "html", "lcov"],
  "exclude": [
    "**/*.test.js",
    "**/*.test.ts",
    "**/*.spec.js",
    "**/*.spec.ts",
    "**/test/**",
    "**/tests/**",
    "**/__tests__/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/coverage/**"
  ]
}
